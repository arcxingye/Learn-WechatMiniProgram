.content{
  background-color: #f2f2f2;
  height: 600px;
}
.line{
  height: 10px;
}
.wrapper{
  border: 1px solid #cccccc;
  width: 90%;
  height: 38px;
  margin:0 auto;
  display: flex;
  flex-direction: row;
  border-radius: 30px;
  align-items: center;
  background-color: #FAFAFA;
}
.search{
  margin-left: 10px;
}
.title{
  display: flex;
  flex-direction: row;
  height: 45px;
  line-height: 45px;
  width: 100%;
  background-color: #D9D9D9;
}
.pre{
  width:30%;
  margin-left: 10px;
  font-weight: bold;
}
.last{
  width:70%;
  text-align: right;
  font-size: 13px;
  margin-right: 10px;
  color: #999999;
}
.menu{
  display: flex;
  flex-direction: row;
  margin: 10px;
}
.btn{
  width: 45%;
  background-color: #EFEEEC;
}